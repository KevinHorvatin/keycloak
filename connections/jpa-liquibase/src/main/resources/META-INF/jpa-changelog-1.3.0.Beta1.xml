<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="dane.barentine@software.dell.com" id="1.3.0.Beta1">
        <createTable tableName="ORGANIZATION">
            <column name="ID" type="VARCHAR(36)">
                <constraints nullable="false"/>
            </column>
            <column name="NAME" type="VARCHAR(255)"/>
            <column name="DESCRIPTION" type="CLOB"/>
            <column defaultValueBoolean="false" name="ENABLED" type="BOOLEAN(1)">
                <constraints nullable="false"/>
            </column>
            <column name="REALM_ID" type="VARCHAR(36)"/>
        </createTable>
        <addPrimaryKey columnNames="ID" constraintName="CONSTRAINT_ORG_ID" tableName="ORGANIZATION"/>
        <addForeignKeyConstraint baseColumnNames="REALM_ID" baseTableName="ORGANIZATION" constraintName="CONSTRAINT_D0" deferrable="false" initiallyDeferred="false" onDelete="RESTRICT" onUpdate="RESTRICT" referencedColumnNames="ID" referencedTableName="REALM"/>
        <customChange class="org.keycloak.connections.jpa.updater.liquibase.custom.JpaUpdate1_3_0_Beta1"/>
    </changeSet>
</databaseChangeLog>
